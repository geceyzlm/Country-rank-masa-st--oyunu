{
  "name": "country-rank-masaustu",
  "version": "1.0.0",
  "description": "Country Rank Desktop Game",
  "author": "alige",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --publish=never"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.example.countryrank",
    "files": [
      "main.js",
      "app/**/*"
    ],
    "win": {
      "target": [
        "portable",
        "nsis"
      ]
    },
    "directories": {
      "output": "dist"
    }
  }
}